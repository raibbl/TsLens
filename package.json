{
  "name": "typescriptlens",
  "displayName": "TypescriptLens",
  "description": "Track typescript progress and reccomend files to convert",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:typescriptlens.helloWorld"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "typescriptlens.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "typescriptProgress.openFile",
        "title": "Open File"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "typescriptLensContainer",
          "title": "TypescriptLens",
          "icon": "$(file-code)"
        }
      ]
    },
    "views": {
      "typescriptLensContainer": [
        {
          "id": "typescriptLensView",
          "name": "TypeScript Progress"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.96.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "eslint": "^9.16.0",
    "typescript": "^5.7.2",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
